<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.testwebflow.entities.Projects" table="Projects" schema="TaskTracker">
        <id name="idProject" column="id_project"/>
        <property name="idAuthor" column="id_author"/>
        <property name="name" column="name"/>
    </class>
</hibernate-mapping>